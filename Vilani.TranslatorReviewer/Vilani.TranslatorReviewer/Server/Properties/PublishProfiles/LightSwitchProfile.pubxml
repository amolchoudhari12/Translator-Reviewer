<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <WebPublishMethod>Package</WebPublishMethod>
    <UseLightSwitchWebDeploymentProvider>True</UseLightSwitchWebDeploymentProvider>
    <AuthenticationType>None</AuthenticationType>
    <RequiresIntrinsicDataSource>true</RequiresIntrinsicDataSource>
    <DeployDatabase>False</DeployDatabase>
    <PackageLocation>../../../../../../../../../VilaniMart%20Releases/Deployables/LanguageReviewer//Vilani.TranslatorReviewer.zip</PackageLocation>
    <CleanWPPAllFilesInSingleFolder>False</CleanWPPAllFilesInSingleFolder>
    <ExcludeGeneratedDebugSymbol>true</ExcludeGeneratedDebugSymbol>
    <DisableAllVSGeneratedMSDeployParameter>True</DisableAllVSGeneratedMSDeployParameter>
    <AutoParameterizationWebConfigConnectionStrings>True</AutoParameterizationWebConfigConnectionStrings>
    <LightSwitchExcludeSetParameter>{Update web.config connection string};{LightSwitchApplication_Path}</LightSwitchExcludeSetParameter>
    <WPPAllFilesInSingleFolder>../Bin/Release/app.publish/</WPPAllFilesInSingleFolder>
    <PackageSourceManifest>../Bin/Release/Vilani.TranslatorReviewer.xml</PackageSourceManifest>
    <ProjectParametersXMLFile>..//Server/generated.parameters.xml</ProjectParametersXMLFile>
    <LightSwitchSqlScriptFile>../Bin/Release/Vilani.TranslatorReviewer.sql</LightSwitchSqlScriptFile>
  </PropertyGroup>
  <Target Name="BeforeBuild">
    <MSBuild Projects="../Vilani.TranslatorReviewer.lsproj" Targets="BeforeLightSwitchPublishApplication" />
  </Target>
</Project>